{
    "name": "go-iam-typescript-example",
    "version": "1.0.0",
    "description": "TypeScript example for Go IAM authentication",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "client": "ts-node src/client/httpclient.ts",
        "clean": "rimraf dist"
    },
    "keywords": [
        "go-iam",
        "authentication",
        "jwt",
        "express",
        "typescript"
    ],
    "dependencies": {
        "express": "^4.18.2",
        "@goiam/typescript": "^0.0.3",
        "dotenv": "^16.3.1",
        "axios": "^1.6.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0",
        "ts-node": "^10.9.0",
        "ts-node-dev": "^2.0.0",
        "rimraf": "^5.0.5"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}